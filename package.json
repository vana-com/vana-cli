{
  "name": "@vana/cli",
  "version": "0.0.1-alpha.0",
  "description": "[ALPHA] Vana CLI for interacting with the Vana L1 network and related services - experimental software",
  "main": "dist/cli.js",
  "bin": {
    "vana": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/cli.ts --format esm --dts --minify",
    "dev": "npm run build && npm link",
    "unlink": "npm unlink -g",
    "lint": "biome lint src",
    "format": "biome format src --write",
    "check": "biome check .",
    "precommit": "biome lint --apply",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vana-com/vana-cli.git"
  },
  "keywords": [
    "vana",
    "cli",
    "blockchain",
    "web3",
    "datadao",
    "data-sovereignty",
    "alpha",
    "experimental"
  ],
  "author": "Vana",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/vana-com/vana-cli/issues"
  },
  "homepage": "https://github.com/vana-com/vana-cli#readme",
  "dependencies": {
    "chalk": "^5.4.1",
    "clipanion": "^4.0.0-rc.4",
    "ethers": "^6.14.4",
    "keytar": "^7.9.0",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "^24.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "semantic-release": "^24.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  }
}
